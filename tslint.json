{
    "defaultSeverity": "error",
    "extends": ["tslint-xo", "tslint-config-prettier"],
    "rulesDirectory": ["tslint-plugin-prettier"],
    "rules": {
        "semicolon": [true, "always"],
        "quotemark": [true, "single"],
        "type-literal-delimiter": false,
        "member-ordering": [
            false
        ],
        "prettier": true
    }
}